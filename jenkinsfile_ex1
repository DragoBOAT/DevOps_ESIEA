pipeline {
    /* I am a comment */
    agent any //who launches pipeline

// All steps to build a CI/CD
    stages { 
        stage('HELLO') {
            steps {
                echo 'Hello World'
            }
        }
        stage('BUILD') {
            steps {
                echo 'Developpement'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing'
            }
        }
        stage('DEPLOY') {
            steps {
                echo 'Mise en Production'
            }
        }
    }
}
